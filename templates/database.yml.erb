production:
  adapter: mysql
  database: <%= scope.lookupvar('redmine::production_db') %>
  username: <%= scope.lookupvar('dbuser') %>
  password: <%= scope.lookupvar('dbpass') %>
  host: localhost
  encoding: utf8

development:
  adapter: mysql
  database: <%= scope.lookupvar('redmine::devel_db') %>
